{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["es5", "es6", "dom"],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "."
  }
}
